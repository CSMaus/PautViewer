<UserControl x:Class="PAUTViewer.Views.MenuUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PAUTViewer.Views"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="800">
    <Grid>
        <Button x:Name="btnExplorer" Width="40" Height="40" 
                Margin="1" Click="LoadViaExplorer_Click" 
            ToolTip="{DynamicResource tooltipOpenExplorer}" >
            <Image Source="Icons/folder.png"/>
        </Button>
    </Grid>
</UserControl>
